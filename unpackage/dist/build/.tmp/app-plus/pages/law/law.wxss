.law {
  width: 100%;
  height: 100%;
  background-color: #f7f7fa;
}
.law .law-item {
  width: 750rpx;
  height: 100rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #F7F7FA;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
}
.law .law-item:last-child {
  border-bottom: none;
}
.law .law-item .item-left {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #09023E;
  letter-spacing: -0.34px;
}
.law .law-item .item-left text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: rgba(9, 2, 0, 0.3);
  letter-spacing: -0.34px;
  line-height: 20px;
}
.law .law-item image {
  width: 16rpx;
  height: 26rpx;
}

