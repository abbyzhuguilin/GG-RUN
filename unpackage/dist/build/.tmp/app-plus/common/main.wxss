/*\*\/ */
page{
	width: 100%;
	height: 100%;
}
/* 顶部阴影 */
page::after {
	content: "";
	position: fixed;
	top: 0;



	left: 0;
	-webkit-box-shadow: inset 0 4rpx 8rpx 0 rgb(244, 244, 244);
	        box-shadow: inset 0 4rpx 8rpx 0 rgb(244, 244, 244);
	z-index: 999;
	width: 750rpx;
	height: 8rpx;
}
.uni-system-preview-image {
		display: block;
		position: fixed;
		left: 0;
		top: 0;
		z-index: 999;
		width: 100%;
		height: 100%;
		background: #000;
}


