<view class="register"><view class="input-box"><input type="number" maxlength="11" placeholder="请输入手机号" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);letter-spacing: -0.34px;" data-event-opts="{{[['input',[['__set_model',['','userPhone','$event',[]]]]]]}}" value="{{userPhone}}" bindinput="__e"/></view><view class="code-box"><input class="register-oode" type="text" placeholder="请输入验证码" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);letter-spacing: -0.34px;" data-event-opts="{{[['input',[['__set_model',['','code','$event',[]]]]]]}}" value="{{code}}" bindinput="__e"/><view data-event-opts="{{[['tap',[['getCode',['$event']]]]]}}" class="{{['get-code',(computedTimer)?'timer':'']}}" bindtap="__e">{{getYzm?'获取验证码':computedTimer+'s'}}</view></view><block wx:if="{{!wxlogin}}"><view class="input-box"><input class="register-item" type="password" placeholder="请输入密码" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);letter-spacing: -0.34px;" data-event-opts="{{[['input',[['__set_model',['','userPwd','$event',[]]]]]]}}" value="{{userPwd}}" bindinput="__e"/></view></block><block wx:if="{{!wxlogin}}"><view class="input-box"><input class="register-item" type="password" placeholder="请重复密码" placeholder-style="font-family: PingFangSC-Regular;font-size: 14px;color: rgba(9,2,62,0.30);letter-spacing: -0.34px;" data-event-opts="{{[['input',[['__set_model',['','userSecPwd','$event',[]]]]]]}}" value="{{userSecPwd}}" bindinput="__e"/></view></block><view class="register-center"><view data-event-opts="{{[['tap',[['handleIsReader',['$event']]]]]}}" class="{{['register-circle',(isReader)?'on':'']}}" bindtap="__e"></view><view data-event-opts="{{[['tap',[['handleIsReader',['$event']]]]]}}" class="register-agree" bindtap="__e">已阅读并同意</view><view data-event-opts="{{[['tap',[['goToRichText',['$event']]]]]}}" class="register-protocol" bindtap="__e">《平台用户协议》</view></view><view data-event-opts="{{[['tap',[['formSubmit',['$event']]]]]}}" class="register-submit" bindtap="__e">确定</view></view>