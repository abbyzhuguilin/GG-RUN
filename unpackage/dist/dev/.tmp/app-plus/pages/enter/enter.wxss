}
.enter .enter-scroll .scroll-item::after {
  content: '';
  width: 0;
  height: 0;
  border-bottom: 4rpx solid #5468FF;
  position: absolute;
  bottom: 0;
  left: 50%;
  -webkit-transform: translateX(-50%);
          transform: translateX(-50%);
  -webkit-transition: 0.5s;
  transition: 0.5s;
}
