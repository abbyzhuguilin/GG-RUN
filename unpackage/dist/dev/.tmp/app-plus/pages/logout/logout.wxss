.logout {
  padding: 30rpx;
  box-sizing: border-box;
}
.logout .logout-title {
  font-family: PingFangSC-Semibold;
  font-size: 17px;
  font-weight: bold;
  color: #09023E;
  letter-spacing: -0.41px;
}
.logout .font-box {
  margin: 30rpx 0;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #09023E;
  letter-spacing: -0.34px;
}
.logout .font-box view {
  margin-top: 20rpx;
  box-sizing: border-box;
}
.logout .logout-bottom {
  width: 100%;
  height: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.logout .logout-bottom .circle {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  border: 1rpx solid #ccc;
  box-sizing: border-box;
}
.logout .logout-bottom .reader {
  margin-left: 20rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: rgba(9, 2, 0, 0.3);
  line-height: 20px;
}
.logout .logout-bottom .protocol {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #5569ff;
  line-height: 20px;
}
.logout .logout-bottom image {
  width: 32rpx;
  height: 32rpx;
}
.logout .next {
  width: 100%;
  height: 90rpx;
  text-align: center;
  line-height: 90rpx;
  background: #5468FF;
  border-radius: 2px;
  box-sizing: border-box;
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #FFFFFF;
  position: fixed;
  left: 0;
  bottom: 0;
}

