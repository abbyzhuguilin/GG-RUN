page {
  background-color: #f7f7fa;
}
.service .service-item {
  padding: 40rpx 0 30rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #F7F7FA;
  background-color: #fff;
}
.service .service-item:last-child {
  border-bottom: none;
}
.service .service-item .item-header {
  font-family: PingFangSC-Semibold;
  font-size: 17px;
  color: #09023E;
  letter-spacing: -0.41px;
  font-weight: bold;
}
.service .service-item .item-text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #09023E;
  letter-spacing: -0.34px;
  margin-top: 20rpx;
  box-sizing: border-box;
}
.service .law-item {
  width: 750rpx;
  height: 100rpx;
  background-color: #fff;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding: 0 30rpx;
  border-bottom: 1rpx solid #F7F7FA;
  box-sizing: border-box;
}
.service .law-item:last-child {
  border-bottom: none;
}
.service .law-item .item-left {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: #09023E;
  letter-spacing: -0.34px;
}
.service .law-item .item-left text {
  font-family: PingFangSC-Regular;
  font-size: 14px;
  color: rgba(9, 2, 0, 0.3);
  letter-spacing: -0.34px;
  line-height: 20px;
}
.service .law-item image {
  width: 16rpx;
  height: 26rpx;
}

