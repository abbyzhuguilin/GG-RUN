<view class="myInfo"><view data-event-opts="{{[['tap',[['handleChoiceAvatar',['$event']]]]]}}" class="myInfo-header" bindtap="__e"><view class="myInfo-avatar"><image src="{{user.avatar?user.avatar:'../../static/img/order/avatar.png'}}" mode="aspectFill" data-event-opts="{{[['tap',[['previewImage',['$event']]]]]}}" catchtap="__e"></image></view><view class="myInfo-right"><view>修改头像</view><image src="../../static/img/my/right.png" mode="aspectFill"></image></view></view><view class="myInfo-item"><view class="item-left">昵称</view><view class="item-right"><input data-event-opts="{{[['blur',[['changeUserName',['$event']]]],['input',[['__set_model',['$0','userName','$event',[]],['user']]]]]}}" value="{{user.userName}}" bindblur="__e" bindinput="__e"/><image src="../../static/img/my/right.png" mode="aspectFill"></image></view></view><view class="myInfo-item"><view class="item-left">性别</view><view class="item-right"><picker range="{{sexList}}" data-event-opts="{{[['change',[['selectedSex',['$event']]]]]}}" bindchange="__e"><input disabled placeholder="请选择" data-event-opts="{{[['input',[['__set_model',['$0','sex','$event',[]],['user']]]]]}}" value="{{user.sex}}" bindinput="__e"/></picker><image src="../../static/img/my/right.png" mode="aspectFill"></image></view></view><view class="myInfo-item"><view class="item-left">生日</view><view class="item-right"><picker mode="date" data-event-opts="{{[['change',[['bindDateChange',['$event']]]]]}}" bindchange="__e"><input disabled placeholder="请选择" data-event-opts="{{[['input',[['__set_model',['$0','birthday','$event',[]],['user']]]]]}}" value="{{user.birthday}}" bindinput="__e"/></picker><image src="../../static/img/my/right.png" mode="aspectFill"></image></view></view><view data-event-opts="{{[['tap',[['goToBindPhone',['$event']]]]]}}" class="myInfo-item" bindtap="__e"><view class="item-left">我的手机号</view><view class="item-right"><input type="number" maxlength="11" disabled data-event-opts="{{[['input',[['__set_model',['$0','phone','$event',[]],['user']]]]]}}" value="{{user.phone}}" bindinput="__e"/><image src="../../static/img/my/right.png" mode="aspectFill"></image></view></view><view data-event-opts="{{[['tap',[['goToChangePwd',['$event']]]]]}}" class="myInfo-item" bindtap="__e"><view class="item-left">修改密码</view><view class="item-right"><image src="../../static/img/my/right.png" mode="aspectFill"></image></view></view></view>